{
  "name": "end-to-end-starter",
  "version": "1.0.0",
  "description": "The ultimate end to end starter",
  "main": "artifacts/index.js",
  "scripts": {
    "watch": "webpack-dev-server --no-inline --watch --content-base src/ --port 8888",
    "clean": "rimraf dist",
    "start": "npm run watch",
    "test": "node_modules/.bin/karma start",
    "test:e2e": "protractor ./config/protractor.conf.js",
    "test:e2e:specific": "protractor ./config/protractor.conf.js --cucumberOpts.tags='@include'",
    "pree2e": "npm run webdriver:update -- --standalone",
    "protractor": "protractor",
    "debug:e2e": "./node_modules/.bin/protractor ./config/protractor.explorer.js --elementExplorer",
    "webdriver-manager": "webdriver-manager",
    "webdriver:update": "npm run webdriver-manager update"
  },
  "keywords": [
    "test",
    "fun",
    "typescript"
  ],
  "author": "Evan Gillogley <evanjmg@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "4.0.4",
    "@types/chai-as-promised": "7.1.0",
    "@types/node": "7.0.0",
    "@types/selenium-webdriver": "^2.53.35",
    "@types/webpack": "3.0.10",
    "awesome-typescript-loader": "3.2.3",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "chokidar": "1.7.0",
    "chokidar-cli": "1.2.0",
    "concurrently": "3.5.0",
    "css-loader": "0.28.7",
    "cucumber": "1.2.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "html-webpack-plugin": "2.26.0",
    "node-sass": "4.5.3",
    "protractor": "^5.1.1",
    "protractor-cucumber-framework": "4.0.3",
    "raw-loader": "0.5.1",
    "react-addons-test-utils": "15.5.1",
    "react-test-renderer": "^15.6.1",
    "rimraf": "2.6.2",
    "sass-lint": "1.11.1",
    "sass-loader": "6.0.6",
    "sinon": "^3.3.0",
    "source-map-loader": "0.2.1",
    "style-loader": "0.18.2",
    "ts-node": "2.0.0",
    "tslint": "5.7.0",
    "typescript": "2.3.4",
    "webpack": "2.4.1",
    "webpack-dev-middleware": "1.12.0",
    "webpack-dev-server": "2.4.1"
  },
  "engines": {
    "node": "6.9.1"
  },
  "dependencies": {
    "@types/react": "15.0.24",
    "@types/react-dom": "15.5.0",
    "core-js": "2.5.1",
    "jasmine": "^2.8.0",
    "react": "15.5.1",
    "react-dom": "15.5.1",
    "redux": "^3.7.2",
    "ts-helpers": "1.1.2"
  }
}
